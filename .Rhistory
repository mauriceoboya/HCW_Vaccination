# Get world map data
world_map <- map_data("world")
# Specify the Horn of Africa countries
horn_of_africa_countries <- c("Somalia", "Ethiopia", "Eritrea", "Kenya", "Djibouti")
# Filter the map data for the Horn of Africa countries and the region
map_region <- world_map %>%
filter(long > 30 & long < 60 & lat > -10 & lat < 25)
# Extract the map data for the Horn of Africa countries for coloring
horn_of_africa_map <- world_map %>%
filter(region %in% horn_of_africa_countries)
# Extract centroids for labeling the countries
country_centroids <- horn_of_africa_map %>%
group_by(region) %>%
summarize(long = mean(range(long)), lat = mean(range(lat)))
# Manually create a data frame for water bodies (Indian Ocean, Red Sea)
water_bodies <- data.frame(
region = c("Indian Ocean", "Red Sea"),
long = c(45, 40),  # Longitudes for labeling
lat = c(-5, 15)    # Latitudes for labeling
)
# Plot the map
ggplot() +
# Plot the Horn of Africa countries with different colors
geom_polygon(data = map_region, aes(x = long, y = lat, group = group),
fill = "lightgrey", color = "white") +
geom_polygon(data = horn_of_africa_map, aes(x = long, y = lat, group = group, fill = region), color = "white") +
# Add borders for countries for more clarity
geom_polygon(data = horn_of_africa_map, aes(x = long, y = lat, group = group),
fill = NA, color = "black", size = 0.5) +
# Add text labels for water bodies (Indian Ocean, Red Sea)
geom_text(data = water_bodies, aes(x = long, y = lat, label = region),
size = 5, color = "blue", fontface = "italic") +
# Use a fixed aspect ratio and limit to specific coordinates (bounding box)
coord_fixed(1.3, xlim = c(32, 55), ylim = c(-10, 25)) +
# Set custom fill colors for each country and water bodies
scale_fill_manual(values = c("Somalia" = "#FF6347",
"Ethiopia" = "#4682B4",
"Eritrea" = "#32CD32",
"Kenya" = "#FFD700",
"Djibouti" = "#8A2BE2",
"Indian Ocean" = "#87CEEB",  # Light blue for ocean
"Red Sea" = "#1E90FF"        # Slightly darker blue for sea
)) +
# Add a background for the oceans
theme_void() +
theme(
plot.background = element_rect(fill = "lightblue"),  # Background for the ocean
panel.background = element_rect(fill = "lightblue"), # Ocean fill
plot.title = element_text(size = 20, face = "bold", hjust = 0.5),
plot.subtitle = element_text(size = 14, hjust = 0.5),
legend.position = "right",  # Show legend on the right
legend.title = element_blank(),  # Remove legend title
legend.text = element_text(size = 12),
plot.margin = margin(10, 10, 10, 10),  # Reduce plot margins
legend.key = element_blank()  # Removes the boxes behind legend labels
) +
# Add titles
labs(
subtitle = "Focusing on The Horn of Africa Region"
)
