library(rnaturalearth)
library(sf)  # for handling spatial data
# Download world map data from rnaturalearth
world <- ne_countries(scale = "medium", returnclass = "sf")
# Define Horn of Africa countries and adjacent regions
horn_countries <- c("Eritrea", "Ethiopia", "Djibouti", "Somalia", "Kenya",
"Sudan", "South Sudan", "Uganda", "Tanzania",
"Yemen", "Saudi Arabia", "Oman")
# Filter for these countries
horn_map <- world %>%
filter(admin %in% horn_countries)
# Basic Map Plotting
ggplot(data = horn_map) +
geom_sf(fill = "lightgreen", color = "darkgray") +  # Color the land
coord_sf(xlim = c(30, 60), ylim = c(-15, 30), expand = FALSE) +  # Limit to Horn of Africa region
theme_minimal() +
labs(title = "Horn of Africa and Surrounding Regions",
subtitle = "Recreating a similar map to the uploaded image") +
theme(plot.title = element_text(hjust = 0.5, size = 16, face = "bold"),
plot.subtitle = element_text(hjust = 0.5, size = 12))
# Adding text annotations for water bodies
ggplot(data = horn_map) +
geom_sf(fill = "lightgreen", color = "darkgray") +  # Color the land
coord_sf(xlim = c(30, 60), ylim = c(-15, 30), expand = FALSE) +  # Horn of Africa region
theme_minimal() +
labs(title = "Horn of Africa and Surrounding Regions",
subtitle = "Recreating a similar map to the uploaded image") +
# Label water bodies
annotate("text", x = 50, y = -5, label = "Indian Ocean", color = "blue", size = 6, angle = 45) +
annotate("text", x = 40, y = 18, label = "Red Sea", color = "blue", size = 6, angle = 45) +
annotate("text", x = 48, y = 12, label = "Gulf of Aden", color = "blue", size = 6) +
theme(plot.title = element_text(hjust = 0.5, size = 16, face = "bold"),
plot.subtitle = element_text(hjust = 0.5, size = 12))
